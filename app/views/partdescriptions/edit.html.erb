<% provide(:title, 'Edit partdescription') %>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= bootstrap_form_for(@partdescription, :url => device_product_category_part_partdescription_path(params[:device_id], params[:product_id], params[:category_id], params[:part_id], params[:id])) do |f| %>
      <%= f.alert_message "Please fix the errors below." %>
      
      <%= f.text_field :title %>
      <%= f.text_field :value %>
 
      <%= f.submit "Edit partdescription", class: "btn btn-primary" %>
      <br><br>
      <%= link_to("Cancel", :back, class: "btn btn-primary") %>
    <% end %>
  </div>
</div>